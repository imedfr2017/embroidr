<?xml version="1.0" encoding="utf-8" ?>

<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
		<section name="pes.config" type="Embroidery.Formats.Pes.Configuration, Embroidery" />
	</configSections>
	<log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
		    <layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%date %-5level %logger [%property{NDC}] - %message%newline" />
		    </layout>
		</appender>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Embroidr.log" />
			<appendToFile value="true" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="2048KB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-5level %logger [%property{NDC}] - %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="ALL" />
			<appender-ref ref="RollingFileAppender" />
			<appender-ref ref="ConsoleAppender" />
		</root>
	</log4net>
	<appSettings>
		<add key="RepositoryPath" value="../../../../data/patterns" />
		<add key="SvgPath" value="../../../../data/svg" />
		<add key="IconPath" value="../../../../data/icon" />
		<add key="IndexFilePath" value="../../../../data/index.xml" />
		<add key="DesignFormatsPath" value="../../../Embroider.Common.DesignFormats/build" />
		<add key="FormatXmlOutput" value="true" />
		<add key="IgnoreJumpStitches" value="true" />
	</appSettings>	
